int AbsoluteValue(int n)
{
	int a;
	if(n < 0)
	{
		n= n*-1;
	}	
	return n;
}
int main()
{	
	int a = 0;
	int n = 0;
	scanf("%d",&n);
	a = AbsoluteValue(n);
	printf("%d",a);
}
